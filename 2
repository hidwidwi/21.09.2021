#include <iostream>;
using namespace std;
int main() {
	setlocale(LC_ALL, "Russian");
	int a, b, c;
	cin >> a >> b;
	c = 0;
	if ((a > 12) || (b > 31)) {
		cout << "Ошибка";
	}

	if ((a == 2) && (b > 0)) {
		if (b <= 28) {
			c = 365 - 31;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 3) && (b > 0)) {
		c = 365 - 31 - 28;
		if (b <= 31) {
			c = 365 - 31 - 28;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 4) && (b > 0)) {
		if (b <= 30) {
			c = 365 - 31 - 28 - 31;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 5) && (b <= 31) && (b > 0)) {
		if (b <= 31) {
			c = 365 - 31 - 28 - 31 - 30;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 6) && (b > 0)) {
		if (b <= 30) {
			c = 365 - 31 - 28 - 31 - 30 - 31;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 7) && (b > 0)) {
		if (b <= 31) {
			c = 365 - 31 - 28 - 31 - 30 - 31 - 30;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 8) && (b > 0)) {
		if (b <= 31) {
			c = 365 - 31 - 28 - 31 - 30 - 31 - 30 - 31;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 9) && (b > 0)) {
		if (b <= 30) {
			c = 365 - 31 - 28 - 31 - 30 - 31 - 30 - 31 - 31;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 10) && (b > 0)) {
		if (b <= 31) {
			c = 365 - 31 - 28 - 31 - 30 - 31 - 30 - 31 - 31 - 30;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 11) && (b > 0)) {
		if (b <= 30) {
			c = 365 - 31 - 28 - 31 - 30 - 31 - 30 - 31 - 31 - 30 - 31;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 12) && (b > 0)) {
		if (b <= 31) {
			c = 365 - 31 - 28 - 31 - 30 - 31 - 30 - 31 - 31 - 30 - 31 - 30;
		}
		else {
			cout << "Ошибка";
		}
	}
	if ((a == 1) && (b > 0)) {
		if (b <= 31) {
			c = 365 - b;
		}
		else {
			cout << "Ошибка";
		}
	}
	if (a == 1) {
		cout << c;
	}
	else {
		cout << c - b;
	}
	return 0;
}
